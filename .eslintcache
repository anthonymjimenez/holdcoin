  return (\n    <li>\n      <a className=\"ui a\" href=\"#\" onClick={() => setToggle(!toggleForms)}>\n        {toggleForms ? <h2>Sign In</h2> : <h2>Log In</h2>}\n      </a>\n      {renderForm()}\n      {console.log(auth.user)}\n      <button onClick={() => auth.signout()}>Signout</button>\n    </li>\n  );\n}\n\nexport default LandPage;\n","/Users/anti/learnCode/JS/project4/holdcoin/src/components/Nav.js",["90","91","92"],"/Users/anti/learnCode/JS/project4/holdcoin/src/components/Ledger.js",["93","94","95","96","97","98","99"],"/Users/anti/learnCode/JS/project4/holdcoin/src/routes/PublicRoute.js",["100"],"import { useAuth } from \"../context/use-auth\";\nimport { Route, Redirect } from \"react-router-dom\";\nimport React, { useEffect } from 'react';\nexport default function PublicRoute({ children, ...rest }) {\n    let auth = useAuth()\n    // check for token to avoid re routing when not starting from land page\n    useEffect(() => {\n      (() => {\n    auth.signInFromToken() \n       })();\n    }, []);\n    return (\n      <Route\n        {...rest}\n        render={({ location }) =>\n        !auth.user ? (\n            children\n          ) : (\n            <Redirect\n              to={{\n                pathname: \"/\",\n                state: { from: location }\n              }}\n            />\n            \n          )\n        \n        }\n      />\n    );\n  }","/Users/anti/learnCode/JS/project4/holdcoin/src/components/CryptoCard.js",["101","102","103","104"],"/Users/anti/learnCode/JS/project4/holdcoin/src/components/LoginForm.js",[],"/Users/anti/learnCode/JS/project4/holdcoin/src/containers/BlockLinkContainer.js",["105","106","107"],"import React from \"react\";\nimport BlockLink from \"../components/BlockLink\";\nimport { getUnique, totalSizePerCrypto, totalSpend, financial, averageCost } from \"../utils/utils\";\nimport { useAuth } from \"../context/use-auth\";\n\nconst BlockLinkContainer = ({ cryptos}) => {\n    // \n  let auth = useAuth()\n  const renderLinks = () => \n    cryptos.map((crypto, index) => <BlockLink key={index} crypto={crypto} averageCost={averageCost(auth.user, crypto)} size={totalSizePerCrypto(auth.user,crypto)} />);\n  ;\n  return (\n    <div>\n      {console.log(averageCost(auth.user, cryptos[1]))}\n\n      {renderLinks()}\n    </div>\n  );\n};\nexport default BlockLinkContainer;\n","/Users/anti/learnCode/JS/project4/holdcoin/src/containers/UserContainer.js",["108","109"],"import React from \"react\";\nimport CryptoLinkContainer from \"./CryptoLinkContainer\";\nimport { NavLink } from \"react-router-dom\";\nimport { getUnique } from \"../utils/utils\";\nimport { useAuth } from \"../context/use-auth\";\n\nfunction UserContainer({cryptos}) {\n  console.log(cryptos)\n  return (\n    <>\n      <h3> Your Cryptos </h3>\n      <CryptoLinkContainer cryptos={cryptos} />\n      <NavLink\n        to={{\n          pathname: `/blockfolio`,\n        }}\n      >\n        Blockfolio\n      </NavLink>\n    </>\n  );\n}\n\nexport default UserContainer;\n","/Users/anti/learnCode/JS/project4/holdcoin/src/components/CryptoLink.js",[],"/Users/anti/learnCode/JS/project4/holdcoin/src/components/Blockfolio.js",["110","111","112"],"/Users/anti/learnCode/JS/project4/holdcoin/src/utils/utils.js",[],"/Users/anti/learnCode/JS/project4/holdcoin/src/components/BlockLink.js",["113"],"/Users/anti/learnCode/JS/project4/holdcoin/src/containers/PrivateContainer.js",["114"],"import React, { useEffect, useState } from \"react\";\nimport CryptoContainer from \"./CryptoContainer\";\nimport UserContainer from \"./UserContainer\";\nimport { url, appendUserInfo } from \"../utils/utils\";\nimport { useAuth } from \"../context/use-auth\";\n\nfunction PrivateContainer({ cryptos }) {\n  let [cryptoData, setCryptoData] = useState([]);\n//useAuth grab user use to render user crypto quickinfo\n// render cryptos and only append userinfo in crypto card?\n  let auth = useAuth()\n  useEffect(() => {\n    (async () => {\n      const results = await fetch(url);\n      const data = await results.json();\n\n    //   setUser(await signInUser());\n      //make fetch to user info don't set to hook, just send to appendUserInfo(, userData)\n      setCryptoData(data);\n      appendUserInfo(setCryptoData, auth.user);\n    })();\n  }, []);\n\n  return (\n    <div>\n\n      {console.log(cryptoData)}\n      <UserContainer cryptos={cryptoData.filter((c) => c.userData)} />\n      <CryptoContainer cryptos={cryptoData} />\n    </div>\n  );\n}\n\nexport default PrivateContainer;\n","/Users/anti/learnCode/JS/project4/holdcoin/src/containers/CryptoContainer.js",["115","116"],"import React, { useEffect, useState } from \"react\";\nimport CryptoLinkContainer from \"./CryptoLinkContainer\";\n\nfunction CryptoContainer({cryptos}) {\n  \n\n  return (\n    <>\n    <h3> Explore the market </h3>\n      <CryptoLinkContainer cryptos={cryptos}/>\n    </>\n  );\n}\n\nexport default CryptoContainer;\n","/Users/anti/learnCode/JS/project4/holdcoin/src/components/TransactionForm.js",["117"],"import React, { useState, useEffect } from \"react\";\nimport { financial } from '../utils/utils'\n\nexport default function TransactionForm({ crypto, user, updateBalance }) {\n  const [state, setState] = useState({\n    size: 0.001,\n    side: \"buy\",\n    user_id: user?.id,\n    crypto: {},\n    hold_price: 1000,\n    stop_limit: 200,\n  });\n\n  useEffect(() => {\n    setState({ ...state, user_id: user?.id });\n    setState({\n      ...state,\n      crypto: {\n        name: crypto.name,\n        price: parseFloat(crypto.price),\n        symbol: crypto.symbol,\n        max_supply: parseInt(crypto.max_supply),\n      },\n    });\n  }, [user]);\n\n  const handleChange = (e) => {\n    let val = e.target.value;\n    if (e.target.placeholder === \"side\") {\n      val = e.target.checked ? \"buy\" : \"sell\";\n    } \n    setState({ ...state, [e.target.placeholder]: val });\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    fetch(\"http://localhost:3000/api/v1/transactions\", {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n        Accept: \"application/json\",\n        Authorization: `Bearer ${localStorage.getItem(\"token\")}`,\n      },\n      body: JSON.stringify(state),\n    })\n      .then((data) => data.json())\n      .then(console.log(user))\n      .then(updateBalance)\n      // .then((data) => {\n      //   console.log(data);\n      // })\n      .catch(console.log);\n  };\n\n  // add to css\n  const formDivStyle = {\n    margin: \"auto\",\n    padding: \"20px\",\n    width: \"80%\",\n  };\n\n  return (\n    <div style={formDivStyle}>\n      <h1>Add Crypto to your Blockfolio</h1>\n      <form className=\"ui form\" onSubmit={handleSubmit}>\n        <div className=\"field\">\n          <label>Size</label>\n          <input\n            value={state.size}\n            onChange={handleChange}\n            type=\"number\"\n            placeholder=\"size\"\n          />\n        </div>\n        <h2>Total price: ${financial(state.size * state.crypto.price)}</h2>\n        <div>\n          Buy\n          <label className=\"toggle-switch\">\n            <input\n              type=\"checkbox\"\n              checked={state.side === \"buy\"}\n              onChange={handleChange}\n              placeholder=\"side\"\n              disabled={true}\n            />\n            <span className=\"switch\" />\n          </label>\n        </div>\n        <div className=\"field\">\n          <label>Hold Price</label>\n          <input\n            value={state.hold_price}\n            onChange={handleChange}\n            type=\"number\"\n            placeholder=\"hold_price\"\n          />\n        </div>\n        <div className=\"field\">\n          <label>Stop Limit</label>\n          <input\n            value={state.stop_limit}\n            onChange={handleChange}\n            type=\"number\"\n            placeholder=\"stop_limit\"\n          />\n        </div>\n\n        <button className=\"ui button\" type=\"submit\">\n          Submit\n        </button>\n      </form>\n    </div>\n  );\n}\n",["118","119"],"/Users/anti/learnCode/JS/project4/holdcoin/src/components/UserInfo.js",["120"],"/Users/anti/learnCode/JS/project4/holdcoin/src/routes/PrivateRoute.js",["121","122"],"/Users/anti/learnCode/JS/project4/holdcoin/src/context/use-auth.js",["123","124","125"],{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","replacedBy":"129"},{"ruleId":"130","severity":1,"message":"131","line":20,"column":7,"nodeType":"132","endLine":20,"endColumn":76},{"ruleId":"133","severity":1,"message":"134","line":1,"column":16,"nodeType":"135","messageId":"136","endLine":1,"endColumn":24},{"ruleId":"133","severity":1,"message":"137","line":1,"column":26,"nodeType":"135","messageId":"136","endLine":1,"endColumn":35},{"ruleId":"133","severity":1,"message":"138","line":3,"column":9,"nodeType":"135","messageId":"136","endLine":3,"endColumn":18},{"ruleId":"133","severity":1,"message":"139","line":4,"column":10,"nodeType":"135","messageId":"136","endLine":4,"endColumn":17},{"ruleId":"133","severity":1,"message":"140","line":4,"column":19,"nodeType":"135","messageId":"136","endLine":4,"endColumn":24},{"ruleId":"133","severity":1,"message":"141","line":4,"column":26,"nodeType":"135","messageId":"136","endLine":4,"endColumn":31},{"ruleId":"133","severity":1,"message":"142","line":4,"column":44,"nodeType":"135","messageId":"136","endLine":4,"endColumn":48},{"ruleId":"133","severity":1,"message":"143","line":4,"column":50,"nodeType":"135","messageId":"136","endLine":4,"endColumn":68},{"ruleId":"133","severity":1,"message":"144","line":4,"column":70,"nodeType":"135","messageId":"136","endLine":4,"endColumn":89},{"ruleId":"133","severity":1,"message":"145","line":4,"column":91,"nodeType":"135","messageId":"136","endLine":4,"endColumn":102},{"ruleId":"146","severity":1,"message":"147","line":11,"column":8,"nodeType":"148","endLine":11,"endColumn":10,"suggestions":"1