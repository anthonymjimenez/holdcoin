-dom\";\nimport React, { useEffect } from 'react';\nexport default function PublicRoute({ children, ...rest }) {\n    let auth = useAuth()\n    // check for token to avoid re routing when not starting from land page\n    useEffect(() => {\n      (() => {\n    auth.signInFromToken() \n       })();\n    }, []);\n    return (\n      <Route\n        {...rest}\n        render={({ location }) =>\n        !auth.user ? (\n            children\n          ) : (\n            <Redirect\n              to={{\n                pathname: \"/\",\n                state: { from: location }\n              }}\n            />\n            \n          )\n        \n        }\n      />\n    );\n  }",["86","87"],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/utils/utils.js",[],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/components/CryptoCard.js",["88","89","90","91"],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/components/Ledger.js",[],["92","93"],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/containers/PrivateContainer.js",["94"],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/index.js",[],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/containers/BlockLinkContainer.js",["95","96","97"],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/containers/CryptoContainer.js",["98","99"],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/components/CryptoLink.js",[],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/components/Blockfolio.js",["100","101"],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/routes/PrivateRoute.js",["102","103"],"import { useAuth } from \"../context/use-auth\";\nimport { Route, Redirect } from \"react-router-dom\";\nimport React, { useEffect, useState } from 'react';\nexport default function PrivateRoute({ children, ...rest }) {\n    let auth = useAuth();\n  \n    return (\n      <Route\n        {...rest}\n        render={({ location }) =>\n        auth.user ? (\n            children\n          ) : (\n            <Redirect\n              to={{\n                pathname: \"/public\",\n                state: { from: location }\n              }}\n            />\n            \n          )\n        \n        }\n      />\n    );\n  }","/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/components/TransactionForm.js",["104"],"import React, { useState, useEffect } from \"react\";\nimport { financial } from '../utils/utils'\n\nexport default function TransactionForm({ crypto, user, updateBalance }) {\n  const [state, setState] = useState({\n    size: 0.001,\n    side: \"buy\",\n    user_id: user?.id,\n    crypto: {},\n    hold_price: 1000,\n    stop_limit: 200,\n  });\n\n  useEffect(() => {\n    setState({ ...state, user_id: user?.id });\n    setState({\n      ...state,\n      crypto: {\n        name: crypto.name,\n        price: parseFloat(crypto.price),\n        symbol: crypto.symbol,\n        max_supply: parseInt(crypto.max_supply),\n      },\n    });\n  }, [user]);\n\n  const handleChange = (e) => {\n    let val = e.target.value;\n    if (e.target.placeholder === \"side\") {\n      val = e.target.checked ? \"buy\" : \"sell\";\n    } \n    setState({ ...state, [e.target.placeholder]: val });\n  };\n\n  const handleSubmit = (e) => {\n    e.preventDefault();\n    fetch(\"http://localhost:3000/api/v1/transactions\", {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n        Accept: \"application/json\",\n        Authorization: `Bearer ${localStorage.getItem(\"token\")}`,\n      },\n      body: JSON.stringify(state),\n    })\n      .then((data) => data.json())\n      .then(console.log(user))\n      .then(updateBalance)\n      // .then((data) => {\n      //   console.log(data);\n      // })\n      .catch(console.log);\n  };\n\n  // add to css\n  const formDivStyle = {\n    margin: \"auto\",\n    padding: \"20px\",\n    width: \"80%\",\n  };\n\n  return (\n    <div style={formDivStyle}>\n      <h1>Add Crypto to your Blockfolio</h1>\n      <form className=\"ui form\" onSubmit={handleSubmit}>\n        <div className=\"field\">\n          <label>Size</label>\n          <input\n            value={state.size}\n            onChange={handleChange}\n            type=\"number\"\n            placeholder=\"size\"\n          />\n        </div>\n        <h2>Total price: ${financial(state.size * state.crypto.price)}</h2>\n        <div>\n          Buy\n          <label className=\"toggle-switch\">\n            <input\n              type=\"checkbox\"\n              checked={state.side === \"buy\"}\n              onChange={handleChange}\n              placeholder=\"side\"\n              disabled={true}\n            />\n            <span className=\"switch\" />\n          </label>\n        </div>\n        <div className=\"field\">\n          <label>Hold Price</label>\n          <input\n            value={state.hold_price}\n            onChange={handleChange}\n            type=\"number\"\n            placeholder=\"hold_price\"\n          />\n        </div>\n        <div className=\"field\">\n          <label>Stop Limit</label>\n          <input\n            value={state.stop_limit}\n            onChange={handleChange}\n            type=\"number\"\n            placeholder=\"stop_limit\"\n          />\n        </div>\n\n        <button className=\"ui button\" type=\"submit\">\n          Submit\n        </button>\n      </form>\n    </div>\n  );\n}\n","/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/components/Nav.js",["105","106"],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/components/Sidebar.js",[],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/routes.js",[],"/Users/Heather/Flatiron/code/Phase4/final/mod-4-crypto-app/holdcoin/src/componen